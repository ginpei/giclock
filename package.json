{
  "private": "true",
  "scripts": {
    "build": "browserify -g envify -t vueify -e src/index.js | babili > gh-pages/build/app.js && cp -r ./index.html ./static/ ./gh-pages/",
    "server": "node script/server.js",
    "dev": "watchify -dv -t vueify -p browserify-hmr -e src/index.js -o build/app.js",
    "lint": "eslint --ext js,vue src/",
    "postinstall": "mkdir build || echo OK",
    "start": "npm run lint -s & concurrently \"npm run server -s\" \"npm run dev -s\""
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babili": "^0.1.4",
    "browserify": "^14.4.0",
    "envify": "^4.1.0",
    "node-sass": "^4.5.3",
    "pug": "^2.0.0-rc.2",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vueify": "^9.4.1",
    "vuex": "^2.3.1"
  },
  "devDependencies": {
    "browserify-hmr": "^0.3.5",
    "concurrently": "^3.5.1",
    "eslint": "^4.2.0",
    "eslint-plugin-vue": "^3.6.0",
    "express": "^4.16.2",
    "watchify": "^3.9.0"
  }
}
